---
interface Props {
  img: string;
  alt: string;
  title: string,
  text: string
}

const { img, alt, title, text } = Astro.props
---

<section class="landscape-wrapper">
  <figure class="landscape-figure" data-aos="zoom-in" data-aos-duration="1500">
    <img src={img} alt={alt}>
  </figure>
  <h3 class="landscape-title capital">{title}</h3>
  <p class="landscape-text">{text}</p>
</section>